{{#firstDisplay}}
    <h4>{{#str}} notificationheader, local_csp {{/str}}</h4>
    <p>{{#str}} notificationstart, local_csp {{/str}}</p>
{{/firstDisplay}}

{{#enforcedEvents.length}}
    <p>{{#str}} notificationblocked, local_csp {{/str}}</p>
    <ul>
        {{#enforcedEvents}}
            <li>{{blockedURI}}</li>
        {{/enforcedEvents}}
    </ul>
{{/enforcedEvents.length}}

{{#reportedEvents.length}}
    <p>{{#str}} notificationnotblocked, local_csp {{/str}}</p>
    <ul>
        {{#reportedEvents}}
            <li>{{blockedURI}}</li>
        {{/reportedEvents}}
    </ul>
{{/reportedEvents.length}}